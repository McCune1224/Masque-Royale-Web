package errors

import "github.com/mccune1224/betrayal-widget/views"

templ Error500() {
	@views.Base() {
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<h1>500</h1>
					<p>Unexpected Error Occured</p>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<a href="/">Back to home</a>
				</div>
			</div>
		</div>
	}
}
